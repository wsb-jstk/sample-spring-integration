<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<property name="LOGS" value="./logs"/>
	<appender name="ConsoleEncoder" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd} | %d{HH:mm:ss.SSS} | %thread | %-5p | %-36(%class{0}#%M\(%line\)) | %m%n</pattern>
			<charset>utf8</charset>
		</encoder>
	</appender>
	<root level="info">
		<appender-ref ref="ConsoleEncoder"/>
	</root>
</configuration>
